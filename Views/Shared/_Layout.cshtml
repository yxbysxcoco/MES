<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - MES</title>
    <!--引入兼容全局css-->
    <link href="~/Content/App/reset.css" rel="stylesheet" />
    <!--引入axios-->
    <script src="~/Scripts/App/axios.min.js"></script>
    <!--引入layui-->
    <link href="~/Content/App/layui.css" />
    <script src="~/Scripts/App/layui.js"></script>
    <!--引入葡萄城插件-->
    <link rel="styleSheet" href="~/Content/Spread/gc.spread.sheets.12.1.0.css" />
    <script src="~/Scripts/Spread/gc.spread.sheets.all.12.1.0.min.js"></script>
    <script src="~/Scripts/Spread/gc.spread.sheets.resources.zh.12.1.0.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    <!--引入时间选择器插件-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
    <!--引入echarts-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/echarts/4.2.1/echarts-en.common.min.js"></script>
    <!--引入词云-->
    <script src="https://oisanjavax.github.io/echarts-wordcloud/dist/echarts-wordcloud.min.js"></script>
    <!--引入ExcelApi-->
    <script src="http://oss.sheetjs.com/js-xlsx/xlsx.full.min.js"></script>
    <script src="~/Scripts/App/excel.js"></script>
    <!--下面这个引入必须放在head的最后面，否则引入不成功-->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.Partial("_Toolbar")
    <div class="container body-content">
        @Html.Partial("_Tagbar")
        @RenderBody()
        @Html.Partial("_Footer")
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
